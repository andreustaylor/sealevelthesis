@article{10.1016/j.oceaneng.2020.107013, 
year = {2020}, 
title = {{Accurate tide level estimation: A deep learning approach}}, 
author = {Riazi, Amin}, 
journal = {Ocean Engineering}, 
issn = {0029-8018}, 
doi = {10.1016/j.oceaneng.2020.107013}, 
abstract = {{ Tide is an essential factor in coastal engineering. The relationship between all forces and factors causing tides are very complex. Traditionally, tides are predicted through harmonic analysis, where a tide level at any time instants is estimated based on the past records. Predicting tide levels based on previously recorded data, strongly suits the application of artificial intelligence models such as neural networks. In the literature, the tide level estimation with neural networks has been mostly considered as a curve-fitting problem. As such, for different beaches different networks have been designed and used. Contrary, in this article, a simple and efficient deep neural network that can estimate future tide levels based on forces and factors affecting the tidal range is proposed. The main advantages of the proposed method are that the input layer can be kept as small as possible, the input data is accessible, and as the input data is based on the main causes of tide, one network can be used for different beaches.}}, 
pages = {107013}, 
volume = {198}, 
local-url = {file://localhost/Users/andytaylor/Documents/Papers%20Library/Riazi-Accurate%20tide%20level%20estimation-%20A%20deep%20learning%20approach-2020-Ocean%20Engineering.pdf}
}
