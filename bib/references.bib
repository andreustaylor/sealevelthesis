@phdthesis{Moore:2004tw, 
year = {2004}, 
rating = {0}, 
title = {{Lateral Boundary Conditions in Numerical Ocean Models}}, 
author = {Moore, Stuart Alex}, 
abstract = {{The large-scale wind-driven ocean circulation is found to be sensitive to the choice of both the lateral boundary condition and the method of implementation. Where model boundaries consist of irregular piecewise-constant steps, a classical free-slip like circulation is only maintained when the free-slip condition is implemented using the Vorticity-Divergence form of the viscous stress tensor. The Ghost-Point method is only capable of such a solution when the boundary located relative vorticity is explicitly set to zero. Free-slip solutions in particular, regardless of the method of implementation, are highly sensitive to the orientation of the piecewise-constant steps and we suggest two mechanisms, one physical and one numerical, by which this sensitivity can be understood.In the second part of this thesis, the spurious stress irregular piecewise-constant coastlines exert on the model flow is quantified by numerically modelling impulsive boundary layer development. In the absence of planetary rotation, the Vorticity-Divergence method of implementation is found to be superior to the Ghost-Point method for flows along irregular coastlines, but is shown to be a dangerous choice as the method of implementation when planetary rotation is included. Including planetary rotation induces the formation of jets near the domain boundaries which alter both the across-channel flow profile and the time dependence of the boundary shear stress from 1/t1/2, for non-rotating cases, to 1/t1/4, for rotating cases. We have developed a simple theoretical model of boundary layer development under the influence of rotation that explains these results.Finally, the phenomenon of western boundary current separation is studied and found to be a consistent feature of both the Ghost-Point and Vorticity-Divergence implemented no-slip model solutions. Separation is completely suppressed under Vorticity-Divergence implemented free-slip conditions. Separation is observed under Ghost-Point implemented free-slip conditions, but can be suppressed by explicitly setting the boundary located relative vorticity to zero.}}
}
