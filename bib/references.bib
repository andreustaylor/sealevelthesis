@article{Piecuch:2012gd, 
year = {2012}, 
rating = {0}, 
title = {{Buoyancy-driven interannual sea level changes in the southeast tropical Pacific}}, 
author = {Piecuch, Christopher G and Ponte, Rui M}, 
journal = {Geophysical Research Letters}, 
doi = {10.1029/2012gl051130}, 
abstract = {{It is commonly held that interannual-to-decadal sea level variability patterns mainly represent the ocean's response to wind forcing. This view is based in part on modeling studies of wind-driven sea level changes along the tropical Pacific. However, because buoyancy forcing (and other generating mechanisms) are usually ignored, this paradigm may overemphasize the role of winds. Focusing on the southeast tropical Pacific, we use a data-constrained ocean state estimate to demonstrate that distinct mechanisms\&\#8212;including the ocean's response to buoyancy forcing as well as nonlinear processes\&\#8212;can also contribute to interannual sea level variability. Contrary to the notion that buoyancy-driven sea level changes are dynamically passive, such changes exhibit a strongly nonlocal, dynamically active character, made manifest in westward propagating waves. As similar findings apply elsewhere, accurate modeling of interannual-to-decadal regional sea level changes requires consideration of a variety of forcing mechanisms, including, but not limited to, the winds.}}, 
pages = {L05607}, 
number = {5}, 
volume = {39}, 
language = {English}, 
month = {01}, 
local-url = {file://localhost/Users/andytaylor/Documents/Library.papers3/Piecuch/2012/Geophys._Res._Lett._2012_Piecuch.pdf}
}
