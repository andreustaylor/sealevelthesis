@article{Desai:2006wo, 
year = {2006}, 
rating = {0}, 
title = {{Application of the convolution formalism to the ocean tide potential: Results from the Gravity Recovery and Climate Experiment (GRACE)}}, 
author = {Desai, Shailen D and Yuan, D.-N.}, 
journal = {Journal of Geophysical Research}, 
doi = {10.1029/2005jc003361}, 
url = {http://www.agu.org/pubs/crossref/2006/2005JC003361.shtml}, 
abstract = {{A computationally efficient approach to reducing omission errors in ocean tide potential models is derived and evaluated using data from the Gravity Recovery and Climate Experiment (GRACE) mission. Ocean tide height models are usually explicitly available at a few frequencies, and a smooth unit response is assumed to infer the response across the tidal spectrum. The convolution formalism of Munk and Cartwright (1966) models this response function with a Fourier series. This allows the total ocean tide height, and therefore the total ocean tide potential, to be modeled as a weighted sum of past, present, and future values of the tide-generating potential. Previous applications of the convolution formalism have usually been limited to tide height models, but we extend it to ocean tide potential models. We use luni-solar ephemerides to derive the required tide-generating potential so that the complete spectrum of the ocean tide potential is efficiently represented. In contrast, the traditionally adopted harmonic model of the ocean tide potential requires the explicit sum of the contributions from individual tidal frequencies. It is therefore subject to omission errors from neglected frequencies and is computationally more intensive. Intersatellite range rate data from the GRACE mission are used to compare convolution and harmonic models of the ocean tide potential. The monthly range rate residual variance is smaller by 4\&\#8211;5\%, and the daily residual variance is smaller by as much as 15\% when using the convolution model than when using a harmonic model that is defined by twice the number of parameters.}}, 
pages = {C06023}, 
number = {C6}, 
volume = {111}, 
language = {English}, 
local-url = {file://localhost/Users/andytaylor/Documents/Library.papers3/Desai/2006/J._Geophys._Res._2006_Desai.pdf}
}
